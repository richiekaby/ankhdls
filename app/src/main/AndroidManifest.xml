<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.larntech.ankhdls">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ankhdls">
        <activity
            android:name=".ui.view_gallery.BarGalleryActivity"
            android:exported="false" />
        <activity
            android:name=".ui.view_gallery.GalleryActivity"
            android:exported="false"
            android:theme="@style/Theme.Ankhdls.NoActionBar" />
        <activity
            android:name=".ui.info.InfoActivity"
            android:exported="false"
            android:theme="@style/Theme.Ankhdls.NoActionBar" />
        <activity
            android:name=".ui.bars.BarActivity"
            android:exported="false"
            android:theme="@style/Theme.Ankhdls.NoActionBar" />
        <activity
            android:name=".ui.delicious.DeliciousActivity"
            android:exported="false"
            android:theme="@style/Theme.Ankhdls.NoActionBar" />
        <activity
            android:name=".ui.fashioned.FashionedPieActivity"
            android:exported="false"
            android:theme="@style/Theme.Ankhdls.NoActionBar" />
        <activity
            android:name=".ui.webview.WebviewActivity"
            android:exported="false"
            android:theme="@style/Theme.Ankhdls.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Ankhdls.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>